{#
    Default variables set
-#}
{% set action_vars = { 'position_class': 'pull-lg-left', 'responsive_hidden_class': 'hidden-md-down','solarize': true, 'icon_increase': 'fa-2x' } %}
<!DOCTYPE html>
<html>
    <head>
        <!-- Meta part -->
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <meta http-equiv="x-ua-compatible" content="ie=edge">

        <!-- Title and favicon part -->
        <title>{% block title %}{% block product_title %}Galileo Extension{% endblock product_title %}{% block page_title %} - Welcome !{% endblock page_title %}{% endblock title %}</title>
        <link rel="shortcut icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

        <!-- Stylesheets part -->
        <link rel="stylesheet" href="{{ asset('css/bootstrap-min.css'|asset_version) }}" type="text/css" media="all" />
        <link rel="stylesheet" href="{{ asset('css/site-min.css'|asset_version) }}" type="text/css" media="all" />
        {% block stylesheets %}{% endblock %}

        <!-- Javascripts in the header -->
        <script type="text/javascript" src="{{ asset('js/jquery-min.js'|asset_version) }}"></script>
        {% block javascriptsheader %}{% endblock %}
    </head>
    <body>
        {%  block navigation %}
            {% block navigation_top_bar %}
                <nav class="navbar navbar-fixed-top navbar-dark bg-inverse rbins-navbar">
                    {% block navigation_top_bar_brand %}
                        <!-- Brand -->
                        <header class="col-xs-2 col-md-3 col-lg-2 media rbins-icon-media">
                            <div class="media-left">
                                <a class="navbar-brand" href="{{ url('app_homepage') }}">
                                    {% block branding %}
                                        <img class="media-object logo-icon" src="{{ asset('img/logo.png') }}" alt="Logo Museum" title="Go to Homepage">
                                    {% endblock branding %}
                                </a>
                            </div>
                        </header>
                    {% endblock navigation_top_bar_brand %}
                    {% block navigation_top_bar_lg %}
                        <nav class="nav nav-inline col-md-9 col-lg-10 hidden-md-down container rbins-lg-actions">
                            {% block navigation_top_bar_lg_teams %}
                                {% include "_partials/navigation/teams.html.twig" %}
                            {% endblock navigation_top_bar_lg_teams %}
                            {% block navigation_top_bar_lg_projects %}
                                {% include "_partials/navigation/projects.html.twig" %}
                            {% endblock navigation_top_bar_lg_projects %}
                            {% block navigation_top_bar_lg_ad_sync %}
                                {% include "_partials/navigation/adSync.html.twig" %}
                            {% endblock navigation_top_bar_lg_ad_sync %}
                            {% set action_vars = action_vars|merge({ 'position_class': 'pull-lg-right', 'responsive_hidden_class': 'hidden-lg-down' }) %}
                            {% block navigation_top_bar_lg_logoff %}
                                {% include "_partials/navigation/logOff.html.twig" with action_vars %}
                            {% endblock navigation_top_bar_lg_logoff %}
                            {% block navigation_top_bar_lg_languages %}
                                {{  locale_switcher() }}
                            {% endblock navigation_top_bar_lg_languages %}
                            {% block navigation_top_bar_lg_profile %}
                                {% include "_partials/navigation/profile.html.twig" with action_vars %}
                            {% endblock navigation_top_bar_lg_profile %}
                        </nav>
                    {% endblock navigation_top_bar_lg %}
                    {% block navigation_top_bar_responsive %}
                        <div class="nav-toggles hidden-xl-up">
                            {% block navigation_top_bar_responsive_close %}
                                <div class="navmenu-close">
                                    <button type="button" class="navbar-toggle navbar-toggle-right" data-target=".rbins-menu">
                                        <span class="icon-bar"></span>
                                    </button>
                                </div>
                            {% endblock navigation_top_bar_responsive_close %}
                            {% block navigation_top_bar_responsive_toggle %}
                                <div class="rbins-menu-toggle">
                                    <button type="button" class="navbar-toggle navbar-toggle-right" data-target=".rbins-menu">
                                        <span class="icon-bar"></span>
                                        <span class="icon-bar"></span>
                                        <span class="icon-bar"></span>
                                    </button>
                                </div>
                            {% endblock navigation_top_bar_responsive_toggle %}
                        </div>
                    {% endblock navigation_top_bar_responsive %}
                </nav>
            {% endblock navigation_top_bar %}
            {% block navigation_responsive_menu %}
                <nav class="col-xs-6 col-sm-4 col-md-3 navmenu navmenu-default navmenu-fixed-right offcanvas hidden-xl-up rbins-menu">
                    <a class="navmenu-brand" href="#">{{ block('product_title') }}</a>
                    <ul class="nav navmenu-nav">
                        <li>
                            {% block navigation_responsive_menu_languages %}
