        {% block javascripts %}
            {% block javascript_navigation %}
                <script type="text/javascript">
                    $(document).ready(
                            function() {

                                var toggleClose = function(elem) {
                                    $(elem).fadeToggle('medium', function() {
                                        if ($(this).is(':visible'))
                                            $(this).css('display','inline-block');
                                    });
                                };

                                $(".rbins-menu-toggle button").on(
                                        'click',
                                        function () {
                                            $($(this).data('target')).toggle( "slide", { direction: "right", done: toggleClose($(".navmenu-close")) }, 500);
                                        }
                                );

                                $(".navmenu-close button").on(
                                        'click',
                                        function () {
                                            $(this).parent().fadeToggle('fast');
                                            $($(this).data('target')).toggle( "slide", { direction: "right" }, 500);
                                        }
                                );
                            }
                    );
                </script>
            {% endblock javascript_navigation %}
        {% endblock javascripts %}
