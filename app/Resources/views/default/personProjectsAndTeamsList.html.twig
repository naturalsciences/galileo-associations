{% extends "default/index.html.twig" %}

{% block content_main %}
    <div class="row rbins-listing">
        <ul class="col-xs-12 pagination pagination-sm">
            {% for key,value in groupsLetter %}
                <li class="page-item {% if value.count == 0 %}disabled{% else %}enabled{% endif %} {% if value.selected == 1 %}active{% endif %}"><a class="page-link" href="#">{{ key }}</a></li>
            {% endfor %}
        </ul>
    </div>
{% endblock content_main %}
